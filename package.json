{
  "name": "rmp-ai-assistant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "npx prisma generate",
    "db:studio": "npx dotenv -e .env.local -- npx prisma studio",
    "db:prototype:push": "npx dotenv -e .env.local -- npx prisma db push",
    "db:prototype:reset": "npx dotenv -e .env.local -- npx prisma db reset --force",
    "db:migrate:dev": "npx dotenv -e .env.local -- npx prisma migrate dev",
    "db:migrate:deploy": "npx dotenv -e .env.local -- npx prisma migrate deploy"
  },
